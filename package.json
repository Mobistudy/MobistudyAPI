{
  "name": "MobistudyAPI",
  "author": "Arvin G <arvin.goo@>, Dario Salvi <dario.salvi@eng.ox.ac.uk>",
  "version": "0.1.0",
  "description": "The REST API of Mobistudy",
  "main": "./src/www.mjs",
  "scripts": {
    "start": "node --experimental-modules ./src/www.mjs",
    "devstart": "nodemon --experimental-modules",
    "test:unit": "jest --updateSnapshot",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "test:integrated": "jest --updateSnapshot --verbose true --detectOpenHandles ./test/jest/__integrated__",
    "test:integrated:watch": "jest --watch ./test/jest/__integrated__",
    "test:integrated:watchAll": "jest --watchAll ./test/jest/__integrated__"
  },
  "dependencies": {
    "arangojs": "^6.13.0",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "express-pino-logger": "^4.0.0",
    "googleapis": "^46.0.0",
    "helmet": "^3.21.2",
    "multi-write-stream": "^2.0.1",
    "nodemailer": "^4.6.8",
    "owasp-password-strength-test": "^1.3.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pino": "^5.16.0",
    "rotating-file-stream": "^1.3.9",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "jest-esnext": "0.0.4",
    "node-docker-api": "^1.1.22"
  }
}
